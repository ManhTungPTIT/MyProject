@model List<MyProject.Models.Dtos.KPIsDto>
@{
    Layout = "_Layout";
}
<table class="table table-hover">
    <thead>
        <tr>
            <th>UserName</th>
            <th>KPI</th>
            <th>Evalute</th>
        </tr>
    </thead>
    <tbody>
    @if (Model != null)
    {
        foreach (var user in Model)
        {
            
            <tr>
                <td>@user.EMployeeName</td>
                <td>@user.Kpis</td>
                @if (@user.Kpis > 1000000)
                {
                    <td>Tốt</td>
                }
                else
                {
                    <td>Trung bình</td>
                }
                
            </tr>
        }
    }
    
    
    </tbody>
</table>